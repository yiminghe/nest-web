{
  "name": "nest-web-common",
  "version": "9.0.11",
  "description": "Using nestjs on web",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:yiminghe/nest-web.git"
  },
  "main": "src/index.ts",
  "@pika/pack": {
    "pipeline": [
      [
        "pika-plugin-ts-types",
        {
          "args": [
            "--rootDir",
            "src"
          ],
          "yarn": true,
          "tsconfig": "tsconfig.build.json"
        }
      ],
      [
        "pika-plugin-build-web-babel",
        {
          "format": "cjs",
          "sourcemap": false
        }
      ],
      [
        "pika-plugin-build-web-babel",
        {
          "sourcemap": false
        }
      ],
      [
        "pika-plugin-clean-dist-src"
      ]
    ]
  },
  "devDependencies": {
    "@pika/pack": "^0.5.0",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "rxjs": "7.x"
  },
  "scripts": {
    "pub": "yarn run build && cd pkg && npm publish",
    "build": "pika-pack build"
  },
  "packageManager": "yarn@3.2.0"
}
